      "inversedBy": "organizers"
    },
    "applications": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::application.application",
      "mappedBy": "user"
    },
    "description": {
      "type": "text"
    },
    "organizer": {
      "type": "boolean"
    },
    "mail": {
      "type": "email"
    },
    "avatar": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    }
